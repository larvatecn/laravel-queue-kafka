{
  "name": "larva/laravel-queue-kafka",
  "description": "This is a Laravel queue adapter for the Kafka.",
  "keywords": [
    "queue",
    "kafka",
    "laravel"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-rdkafka": "*",
    "illuminate/database": "^6.0 || ^7.0 || ^8.0",
    "illuminate/support": "^6.0 || ^7.0 || ^8.0",
    "illuminate/queue": "^6.0 || ^7.0 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Queue\\Kafka\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Queue\\Kafka\\KafkaQueueServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
